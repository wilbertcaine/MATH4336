function linear_average_filter()
    A = imread('Fig3.37(a).jpg');
    [row, col] = size(A);
    for i = 2 : row-1
        for j = 2 : col-1
            f = 0
            for s = i-1 : i+1
                for t = j-1 : j+1
                    f = f + A(s, t);
                end
            end
            
end

